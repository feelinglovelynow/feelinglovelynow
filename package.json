{
  "name": "feelinglovelynow",
  "version": "1.0.0",
  "type": "module",
  "main": "./build/index.js",
  "scripts": {
    "write": "bash ./src/lib/scripts/write.sh",
    "devWrite": "env=local host=http://localhost:5173 npm run write",
    "dev": "bash ./src/lib/scripts/dev.sh",
    "mainWrite": "env=main host=https://feelinglovelynow.com npm run write",
    "build": "bash ./src/lib/scripts/mainBuild.sh",
    "preview": "npx vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "types": "node node_modules/@ace/db/src/cli/cli.js types"
  },
  "devDependencies": {
    "@ace/db": "git+https://github.com/acedatabasefoundation/db.git",
    "@feelinglovelynow/datetime-local": "latest",
    "@feelinglovelynow/env-write": "latest",
    "@feelinglovelynow/get-form-entries": "latest",
    "@feelinglovelynow/global-style": "latest",
    "@feelinglovelynow/jwt": "latest",
    "@feelinglovelynow/loop-backwards": "latest",
    "@feelinglovelynow/slug": "latest",
    "@feelinglovelynow/svelte-catch": "latest",
    "@feelinglovelynow/svelte-kv": "latest",
    "@feelinglovelynow/svelte-loading-anchor": "latest",
    "@feelinglovelynow/svelte-modal": "latest",
    "@feelinglovelynow/svelte-turnstile": "latest",
    "@feelinglovelynow/toast": "latest",
    "@paypal/paypal-js": "^7.1.1",
    "@poppanator/sveltekit-svg": "^4.2.1",
    "@sveltejs/adapter-node": "^5.2.2",
    "@sveltejs/kit": "^2.5.24",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/node": "^22.5.0",
    "sass": "^1.77.8",
    "svelte": "4.2.3",
    "svelte-check": "^3.8.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "wrangler": "^3.72.2",
    "zod": "^3.23.8"
  }
}
