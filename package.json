{
  "name": "feelinglovelynow",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "write": "bash ./src/lib/scripts/write.sh",
    "devWrite": "env=local host=http://localhost:5173 pnpm write",
    "dev": "bash ./src/lib/scripts/dev.sh",
    "qaWrite": "env=qa host=https://qa.feelinglovelynow.com pnpm write",
    "qaPush": "bash ./src/lib/scripts/qaPush.sh",
    "qaDeploy": "bash ./src/lib/scripts/qaDeploy.sh",
    "qaLogs": "pnpm wrangler tail --env qa",
    "mainWrite": "env=main host=https://feelinglovelynow.com pnpm write",
    "mainPush": "bash ./src/lib/scripts/mainPush.sh",
    "mainDeploy": "bash ./src/lib/scripts/mainDeploy.sh",
    "mainLogs": "pnpm wrangler tail",
    "preview": "pnpm vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231025.0",
    "@feelinglovelynow/env-write": "^1.0.0",
    "@feelinglovelynow/get-form-entries": "^1.0.0",
    "@feelinglovelynow/global-style": "^1.0.1",
    "@feelinglovelynow/jwt": "^1.0.0",
    "@feelinglovelynow/loop-backwards": "^1.0.0",
    "@feelinglovelynow/slug": "^1.0.0",
    "@feelinglovelynow/svelte-loading-anchor": "^1.0.0",
    "@feelinglovelynow/svelte-modal": "^1.0.1",
    "@feelinglovelynow/svelte-turnstile": "^1.0.0",
    "@feelinglovelynow/toast": "^1.2.0",
    "@paypal/paypal-js": "^7.0.3",
    "@poppanator/sveltekit-svg": "^4.1.3",
    "@sveltejs/adapter-cloudflare-workers": "^1.2.0",
    "@sveltejs/kit": "^1.27.5",
    "@types/node": "^20.9.0",
    "buffer": "^6.0.3",
    "sass": "^1.69.5",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "wrangler": "^3.15.0",
    "zod": "^3.22.4"
  }
}
