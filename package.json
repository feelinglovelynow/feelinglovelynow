{
  "name": "feelinglovelynow",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "write": "bash ./src/lib/scripts/write.sh",
    "devWrite": "env=local host=http://localhost:5173 pnpm write",
    "dev": "bash ./src/lib/scripts/dev.sh",
    "qaWrite": "env=qa host=https://qa.feelinglovelynow.com pnpm write",
    "qaPush": "bash ./src/lib/scripts/qaPush.sh",
    "qaDeploy": "bash ./src/lib/scripts/qaDeploy.sh",
    "qaLogs": "pnpm wrangler tail --env qa",
    "mainWrite": "env=main host=https://feelinglovelynow.com pnpm write",
    "mainPush": "bash ./src/lib/scripts/mainPush.sh",
    "mainDeploy": "bash ./src/lib/scripts/mainDeploy.sh",
    "mainLogs": "pnpm wrangler tail",
    "preview": "pnpm vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231010.0",
    "@poppanator/sveltekit-svg": "^2.1.2",
    "@sensethenlove/env-write": "^1.1.1",
    "@sensethenlove/get-form-entries": "^1.0.1",
    "@sensethenlove/get-relative-time": "^1.0.0",
    "@sensethenlove/global-style": "^1.0.8",
    "@sensethenlove/jwt": "^1.0.6",
    "@sensethenlove/loop-backwards": "^1.0.9",
    "@sensethenlove/slug": "^1.0.2",
    "@sensethenlove/svelte-loading-anchor": "^1.0.2",
    "@sensethenlove/svelte-modal": "^1.0.2",
    "@sensethenlove/svelte-turnstile": "^1.1.5",
    "@sensethenlove/toast": "^1.0.8",
    "@sveltejs/adapter-cloudflare-workers": "^1.1.4",
    "@sveltejs/kit": "^1.25.2",
    "@types/braintree-web": "^3.96.7",
    "@types/braintree-web-drop-in": "^1.39.1",
    "@types/node": "^18.18.5",
    "braintree-web": "^3.97.2",
    "buffer": "^6.0.3",
    "sass": "^1.69.3",
    "svelte": "^3.59.2",
    "svelte-check": "^3.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "wrangler": "^3.12.0",
    "zod": "^3.22.4"
  }
}
