{
  "name": "feelinglovelynow",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "write": "bash ./src/lib/scripts/write.sh",
    "devWrite": "env=local host=http://localhost:5173 pnpm write",
    "dev": "bash ./src/lib/scripts/dev.sh",
    "qaWrite": "env=qa host=https://qa.feelinglovelynow.com pnpm write",
    "qaPush": "bash ./src/lib/scripts/qaPush.sh",
    "qaDeploy": "bash ./src/lib/scripts/qaDeploy.sh",
    "qaLogs": "pnpm wrangler tail --env qa",
    "mainWrite": "env=main host=https://feelinglovelynow.com pnpm write",
    "mainPush": "bash ./src/lib/scripts/mainPush.sh",
    "mainDeploy": "bash ./src/lib/scripts/mainDeploy.sh",
    "mainLogs": "pnpm wrangler tail",
    "preview": "pnpm vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@feelinglovelynow/datetime-local": "latest",
    "@feelinglovelynow/dgraph": "latest",
    "@feelinglovelynow/env-write": "latest",
    "@feelinglovelynow/get-form-entries": "latest",
    "@feelinglovelynow/global-style": "latest",
    "@feelinglovelynow/jwt": "latest",
    "@feelinglovelynow/loop-backwards": "latest",
    "@feelinglovelynow/slug": "latest",
    "@feelinglovelynow/svelte-catch": "latest",
    "@feelinglovelynow/svelte-kv": "latest",
    "@feelinglovelynow/svelte-loading-anchor": "latest",
    "@feelinglovelynow/svelte-modal": "latest",
    "@feelinglovelynow/svelte-turnstile": "latest",
    "@feelinglovelynow/toast": "latest",
    "@paypal/paypal-js": "^7.1.1",
    "@poppanator/sveltekit-svg": "^4.1.3",
    "@sveltejs/adapter-cloudflare-workers": "^2.0.0",
    "@sveltejs/kit": "^2.0.1",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@types/node": "^20.10.5",
    "buffer": "^6.0.3",
    "sass": "^1.69.5",
    "svelte": "4.2.3",
    "svelte-check": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "wrangler": "^3.21.0",
    "zod": "^3.22.4"
  }
}
